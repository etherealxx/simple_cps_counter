config_version=5

[application]

config/name="simple_cps_counter"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
run/max_fps=240
config/icon="res://icon.svg"

[autoload]

GlobalInput="*res://addons/global_input/autoloads/global_input_gdscript/GlobalInputGDScript.tscn"

[display]

window/size/viewport_width=350
window/size/viewport_height=100
window/size/borderless=true
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="simple_cps_counter"

[editor_plugins]

enabled=PackedStringArray("res://addons/global_input/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

